<% if flash[:notice] %>
  <div class="notice">
    <%= flash[:notice] %>
  </div>
<% end %>

<% if logged_in? %>
  Hello, <%= current_user.email %>
<% end %>

<% if logged_in? %>
  <%= link_to "My Resumes", resumes_path %>
<% end %>

<% if logged_in? %>
  <%= link_to 'Logout', logout_path %>
<% else %>
  <%= link_to 'Register', register_path %>
  <br />
  <%= link_to 'Login', login_path %>
<% end %>
