<li class="simpleitem item">
<%= fields_for "resume[simplelists_attributes][#{list.id}][simpleitems_attributes][#{item.id}]", item do |itemf| %>
    <%= itemf.hidden_field :id %>
    <%= itemf.hidden_field :order, class: 'item_order' %>
    <%= itemf.text_field :content, class: 'form-control' %>

    <%= link_to resume_simplelist_simpleitem_path(@resume.id, list.id, item.id), class: 'item_delete' do %>
      <span class="glyphicon glyphicon-trash"></span>
    <% end %>
<% end %>
</li>
