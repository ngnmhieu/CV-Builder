<% list = data %>
<br />
<br />
  <%= rform.fields_for :multiline_lists, list do |lf| %>

    <%= lf.label :name, "List Name" %>
    <%= lf.text_field :name %>
    <br />
    Items (<%=link_to 'Add', resume_multiline_list_multiline_list_items_path(@resume.id, list.id), method: :post %>)
    <% ordered_list = list.multiline_list_items.sort_by {|item| item.order } %>
    <% ordered_list.each do |item| %>
      <%= lf.fields_for :multiline_list_items, item do |itemf| %>
        <table border=1>
          <tr>
            <td><%= itemf.label :line1, 'Line 1' %></td>
            <td><%= itemf.text_field :line1 %></td>
          </tr>
          <tr>
            <td><%= itemf.label :line2, 'Line 2' %></td>
            <td><%= itemf.text_field :line2 %></td>
          </tr>
          <tr>
            <td><%= itemf.label :desc, 'Description' %></td>
            <td><%= itemf.text_area :desc, size: '30x3' %></td>
          </tr>
          <tr>
            <td><%= itemf.label :start, 'Start' %></td>
            <td><%= itemf.date_select :start, discard_day: true, order: [:month, :year] %></td>
          </tr>
          <tr>
            <td><%= itemf.label :end, 'End' %></td>
            <td><%= itemf.date_select :end, discard_day: true, order: [:month, :year] %></td>
          </tr>
          <tr>
            <td><%= link_to 'X', resume_multiline_list_multiline_list_item_path(@resume.id, list.id, item.id), method: :delete%></td>
            <td>
              <% if item.order != 1 %>
                <%= link_to "up", mll_item_incr_order_path(@resume.id, list.id, item.id), method: :post %>
              <% end %>

              <% if item.order != ordered_list.size %>
                <%= link_to "down", mll_item_decr_order_path(@resume.id, list.id, item.id), method: :post %>
              <% end %>
            </td>
          </tr>
        </table>
      <% end %>
    <% end %>
    <%= link_to 'Remove List', resume_multiline_list_path(@resume.id, list.id), method: :delete %>
    <% if list.order != 1 %>
      | <%= link_to 'Up', mll_incr_order_path(@resume.id, list.id), method: :post %>
    <% end %>
    <% if list.order != @resume.items.size %>
      | <%= link_to 'Down', mll_decr_order_path(@resume.id, list.id), method: :post %>
    <% end %>
    <br />

  <% end %>
