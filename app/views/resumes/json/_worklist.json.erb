<% @list = data %>
{
  "name" : "<%= @list.name %>",
  "type" : "worklist",
  "order": <%= @list.order %>,
  "items": [
    <% @list.items.each_with_index do |item, i| %>
      <%= render partial: "resumes/json/workitem.json", locals: {item: item}  %>
      <%= ',' if i != @list.items.size - 1 %>
    <% end %>
  ]
}
